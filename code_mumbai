# Testing basic operations

# 1. Arithmetic Test
def arithmetic_test():
    a = 10
    b = 5
    addition = a + b
    subtraction = a - b
    multiplication = a * b
    division = a / b
    print(f"Addition: {addition}")
    print(f"Subtraction: {subtraction}")
    print(f"Multiplication: {multiplication}")
    print(f"Division: {division}")
